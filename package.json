{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "hexo": {
    "version": "3.7.1"
  },
  "scripts": {
    "dev": "hexo server",
    "add-subtree": "git subtree add --prefix=themes/even https://github.com/ahonn/hexo-theme-even.git master --squash",
    "pull-subtree": "git subtree pull --prefix=themes/even https://github.com/ahonn/hexo-theme-even.git master --squash",
    "init": "npm run add-subtree & npm install",
    "pull": "git pull && npm run pull-subtree",
    "deploy": "hexo g -d"
  },
  "dependencies": {
    "hexo": "^3.8.0",
    "hexo-deployer-git": "^0.3.0",
    "hexo-generator-archive": "^0.1.5",
    "hexo-generator-category": "^0.1.3",
    "hexo-generator-feed": "^1.2.2",
    "hexo-generator-index": "^0.2.0",
    "hexo-generator-json-content": "^3.0.1",
    "hexo-generator-tag": "^0.2.0",
    "hexo-renderer-ejs": "^0.2.0",
    "hexo-renderer-marked": "^0.2.10",
    "hexo-renderer-scss": "^1.2.0",
    "hexo-renderer-stylus": "^0.3.1",
    "hexo-server": "^0.2.0"
  }
}